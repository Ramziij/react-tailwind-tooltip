  "scripts": {
    "clean": "rm -rf dist/",
    "build": "pnpm clean && pnpm build:esm && pnpm build:cjs && pnpm build:css",
    "build:esm": "tsc",
    "build:cjs": "tsc --module commonjs --outDir dist/cjs",
    "build:css": "tailwindcss -o dist/index.css --minify",
    "prepare": "pnpm build"
  },
  "peerDependencies": {
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "keywords": [
    "react",
    "typescript",
    "tailwind",
    "tooltip",
    "react-tooltip",
    "tailwind-tooltip",
    "daisyui"
  ],
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "autoprefixer": "^9.8.6",
    "daisyui": "^4.12.10",
    "postcss": "^8.4.39",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "tailwindcss": "^3.4.4",
    "tslib": "^2.6.3",
    "typescript": "^4.9.5",
    "web-vitals": "^2.1.4"
  },
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts"
}
